{
  "name": "logging-pretty",
  "version": "1.6.8",
  "description": "Logging your message with pretty color with node.js",
  "scripts": {
    "test": "node test/index.js",
    "build:tsup": "rm -rf dist/ && tsup ./src/index.ts --format cjs,esm --dts",
    "build:tsc": "rm -rf dist/ && tsc",
    "publish:npm": "npm run build:tsup && npm publish"
  },
  "homepage": "https://github.com/damartripamungkas/logging-pretty",
  "repository": {
    "type": "git",
    "url": "https://github.com/damartripamungkas/logging-pretty.git"
  },
  "keywords": [
    "console.log",
    "logger",
    "chalk",
    "logging-color",
    "node-console",
    "logging-pretty",
    "logger-pretty",
    "log-pretty",
    "log-beautiful"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "damartripamungkas",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "dayjs": "^1.11.7"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.52.0"
  }
}
